<div id="workout-card">
  <div class="workout-card">
    <div class="workout-card-header">
      <img
          src="./images/best-exercise-for-fibromyalgia.jpg"
          alt="sporting"
          width="100%"
          height="150px"
      />
      {{title}}
    </div>
    <div class="card-body">
      <p><strong>Description:</strong> {{description}}</p>
      <p>
        <strong>Time frame:</strong>
        {{time_frame}}
      </p>
      <p>
        <strong>Level:</strong>
        {{intensity}}
      </p>

      <p>
          <a href="/workout/benchpress" class="btn-save">read more</a>
      </p>
    </div>
  </div>
</div>