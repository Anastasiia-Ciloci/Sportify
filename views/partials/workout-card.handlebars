 <div id="workout-card">
      <div class="workout-card">
        <div class="workout-card-header">
          <img
            src="./images/best-exercise-for-fibromyalgia.jpg"
            alt="sportimg"
            width="100%"
            height="150px"
          />
          {{workouts.title}}
        </div>
        <div class="card-body">
          <!-- <p><strong>Description:</strong> {{description}}</p> -->
          <p>
            <strong>Time frame:</strong>
            {{workouts.time_frame}}
          </p>
          <p>
            <strong>Level:</strong>
            {{workouts.intensity}}
          </p>

          <p><button class="btn-save">read more</button></p>
        </div>
      </div>